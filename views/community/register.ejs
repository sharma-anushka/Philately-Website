<% layout("layouts/boilerplate") %>
<br><br><br>
<form enctype="multipart/form-data">
    <div class="mb-3">
      <label for="name" class="form-label">Name</label>
      <input type="text" class="form-control" id="name" aria-describedby="emailHelp">
    </div>
    <div class="mb-3">
      <label for="age" class="form-label">Age</label>
      <input type="date" class="form-control" id="age">
    </div>
    <div class="mb-3">
        <label for="Phone Number" class="form-label">Phone Number</label>
        <input type="Number" class="form-control" id="Phone Number">
    </div>
    <div class="mb-3">
        <label for="file" class="form-label">Upload a Government ID (Aadhar/ driving liscence) </label>
        <input type="file" id="file" name="file" accept=".pdf,.doc,.docx,.txt" required>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
  </form>

  